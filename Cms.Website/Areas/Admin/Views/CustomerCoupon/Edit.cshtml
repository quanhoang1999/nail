
@model CustomerCouponViewModel
@{
    ViewData["Title"] = "Nail customer";
}
@section Styles{
    <link href="~/lib/jQuery-EasyUI/themes/bootstrap/easyui.css" rel="stylesheet" />
    <link href="~/lib/jQuery-EasyUI/themes/icon.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-tagsinput/bootstrap-tagsinput.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-datepicker/css/bootstrap-datepicker.standalone.min.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
}
@section Scripts{
    <script src="~/lib/bootstrap-tagsinput/bootstrap-tagsinput.js"></script>
    <script src="~/lib/jQuery-EasyUI/jquery.easyui.min.js"></script>
    <script src="~/lib/moment/moment.js"></script>
    <script src="~/lib/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>

    <script src="~/lib/bootstrap-select/js/bootstrap-select.min.js"></script>
    <script src="~/app/controllers/nailcoupon/detail.js" asp-append-version="true"></script>
    <script>

        var review = new NailCouponEditController();
        review.initialize();
    </script>


}




<input type="hidden" id="hid-category-id" value="0" />
<div class="">
    <div class="row">
        <div class="col-md-6">
            <h3>
                Coupon
                <small>
                    <i class="fa fa-arrow-circle-left"></i>
                    <a href="/Admin/customercoupon">Back</a>
                </small>
            </h3>

        </div>
        <div class="col-md-6 text-right">



        </div>
    </div>


    <div class="clearfix"></div>
    <div class="x_panel">
        <div class="x_content">
            <div class="row">
                <div class="col-lg-6">
                    <form class="form-horizontal" role="form" id="frmMaintainance">
                        <div class="form-group row">
                            <input type="hidden" id="hidIdM" asp-for="Id" />
                            <label for="txtNameM" class="col-sm-2 control-label no-padding-right">Customer</label>
                            <div class="col-sm-10">
                                <select data-live-search="true" asp-items="@(new SelectList(ViewBag.AllCustomer,"Id","FullName"))" id="slCustomer">
                                </select>

                            </div>
                        </div>
                        <div class="form-group row">

                            <label for="txtDateStarted" class="col-sm-2 control-label no-padding-right">Date Started</label>
                            <div class="col-sm-4">
                                <div class="input-group date" data-provide="datepicker">
                                    <input type="text" readonly asp-for="DateStarted" id="txtDateStarted" class="form-control">
                                    <div class="input-group-append">
                                        <span class="input-group-text">
                                            <i class="far fa-calendar-alt"></i>
                                        </span>
                                    </div>
                                </div>

                            </div>
                            <label for="txtDateExpired" class="col-sm-2 control-label no-padding-right">Date Expired</label>
                            <div class="col-sm-4">
                                <div class="input-group date" data-provide="datepicker">
                                    <input type="text" readonly asp-for="DateExpired" id="txtDateExpired" class="form-control">
                                    <div class="input-group-append">
                                        <span class="input-group-text">
                                            <i class="far fa-calendar-alt"></i>
                                        </span>
                                    </div>
                                </div>

                            </div>
                        </div>


                        <div class="form-group row">
                            <label asp-for="DiscountType" class="col-sm-2 control-label no-padding-right"></label>
                            <div class="col-sm-10">
                                <select asp-for="DiscountType" id="slDiscountType">
                                    <option value="1">Percent</option>
                                    <option value="2">Money</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">

                            <label asp-for="Discount" class="col-sm-2 control-label no-padding-right">Discount</label>
                            <div class="col-sm-10">
                                <input type="text" asp-for="Discount" class="form-control" id="txtDiscount">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label asp-for="NailStoreId" class="col-sm-2 control-label no-padding-right">Store</label>
                            <div class="col-sm-10">
                                <select asp-for="NailStoreId" id="slNailStoreId" asp-items="@(new SelectList(ViewBag.NailStore,"Id","Name"))">
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-2"></div>
                            <div class="col-sm-10">
                                <button type="button" id="btnSave" class="btn btn-primary">Save changes</button>
                                <button type="button" id="btnCancel" data-dismiss="modal" class="btn btn-secondary">Cancel</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>


</div>
<partial name="_SearchCustomerModal" />


