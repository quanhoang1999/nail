@model List<GalleryViewModel>
@if (Model != null && Model.Count() > 0)
{<div class="showroom-slide store-gallery-index">
        <div class="main-slider-showroom">

            <div id="owl-home-main-slider-showroom" class="owl-home-main-slider-showroom">
                @foreach (var item in Model)
                {
                    <div class="owl-item">
                        <div class="item">
                            <a href="/store/@item.NailStoreId">
                                <div class="item-image"
                                     style="background-image: url(@item.FileFullUrl);">
                                </div>
                            </a>
                        </div>
                    </div>
                }
            </div>
        </div>

    </div>

}

<script type="text/javascript" src="~/landingpage/js/popper.min.js"></script>
<script type="text/javascript" src="~/landingpage/vender/owlcarousel/js/owl.carousel.min.js"></script>
<script type="text/javascript" src="~/landingpage/js/lixilshowroom.js"></script>
